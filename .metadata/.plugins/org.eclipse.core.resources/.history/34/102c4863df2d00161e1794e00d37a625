package trabalho;

import estruturas.Promissoria;

public class TabelaHashing {
	private Promissoria[] vetor;
	private int nElem;

	public TabelaHashing() {
		this.vetor = null;
		this.nElem = 0;
	}

	private int calculaTamanho(int qtd) {
		int tamanho = qtd + 1;
		while (!ÈPrimo(tamanho)) {
			
			
		}
		for (int i = vetor.length; i < vetor.length; i++) {
			int resultado = qtd % i;
			if (resultado == 0) {
				qtd++;
				i = 1;
			}
		}

		return 0;
	}

	private Boolean ÈPrimo(int numero) {
		// se o n˙mero È par, n„o pode ser primo
		if (numero % 2 == 0) {
			return false;
		}
		// verificar se este n˙mero Ìmpar È primo
		for (long i = 3; i * i <= numero; i += 2) {
			if (numero % i == 0) {
				return false;
			}
		}
		return true;
	}
}
